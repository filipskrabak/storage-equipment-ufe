{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "generator-cli": {
      "useDocker": true,
      "version": "6.6.0",
      "generators": {
          "storage-equipment": {
              "generatorName": "typescript-fetch",
              "glob": "api/storage-equipment.openapi.yaml",
              "output": "#{cwd}/src/api/storage-equipment",
              "additionalProperties": {
                  "supportsES6": "true",
                  "withInterfaces": true
              }
          }
      }
  }
}
